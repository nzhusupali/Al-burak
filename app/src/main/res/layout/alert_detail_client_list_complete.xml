<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:padding="10dp"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView"
        style="@style/TitleText_detailClient"
        android:layout_marginTop="20dp"
        android:text="@string/employee_name1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:drawableStartCompat="@drawable/ic_engineering" />

    <TextView
        android:id="@+id/textView2"
        style="@style/TitleText_detailClient"
        android:text="@string/CarBrand1"
        app:drawableStartCompat="@drawable/ic_car"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        style="@style/TitleText_detailClient"
        android:text="@string/StateNumberCar1"
        app:drawableStartCompat="@drawable/ic_country"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        style="@style/TitleText_detailClient"
        android:text="@string/FinalSum1"
        app:drawableStartCompat="@drawable/ic_cash"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        style="@style/TitleText_detailClient"
        android:text="@string/phone_number1"
        app:drawableStartCompat="@drawable/ic_phone"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        style="@style/TitleText_detailClient"
        android:text="@string/client_name1"
        app:drawableStartCompat="@drawable/ic_person"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView14"
        style="@style/TitleText_detailClient"
        android:text="@string/datePicker"
        app:drawableStartCompat="@drawable/ic_date"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView12"
        style="@style/TitleText_detailClient"
        android:text="@string/Notes1"
        app:drawableStartCompat="@drawable/ic_task_build"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/employee_cng"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="Alihan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/carName_cng"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="Mercedes-benz"
        app:layout_constraintEnd_toEndOf="@+id/employee_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/employee_cng" />

    <TextView
        android:id="@+id/stateNumber_cng"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="KZ063AMG01"
        app:layout_constraintEnd_toEndOf="@+id/carName_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/carName_cng" />

    <TextView
        android:id="@+id/sum_cng"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="100,000"
        app:layout_constraintEnd_toEndOf="@+id/stateNumber_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/stateNumber_cng" />

    <TextView
        android:id="@+id/phoneNumber_cng"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="89798516545"
        app:layout_constraintEnd_toEndOf="@+id/sum_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/sum_cng" />

    <TextView
        android:id="@+id/clientName_cng"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="Shevtsov"
        app:layout_constraintEnd_toEndOf="@+id/phoneNumber_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/phoneNumber_cng" />

    <TextView
        android:id="@+id/datePicker_cng"
        style="@style/changeText_detailClient"
        android:text="19 july 2021"
        app:layout_constraintEnd_toEndOf="@+id/clientName_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/clientName_cng" />

    <TextView
        android:id="@+id/detail_cng"
        style="@style/changeText_detailClient"
        android:text="Engine, chassis"
        app:layout_constraintEnd_toEndOf="@+id/datePicker_cng"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/datePicker_cng" />
</androidx.constraintlayout.widget.ConstraintLayout>