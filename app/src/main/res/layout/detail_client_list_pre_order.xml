<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:padding="10dp"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView2"
        style="@style/TitleText_detailClient"
        android:layout_marginTop="20dp"
        android:text="@string/CarBrand1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:drawableStartCompat="@drawable/ic_car" />
    <TextView
        android:id="@+id/textView6"
        style="@style/TitleText_detailClient"
        android:text="@string/client_name1"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:drawableStartCompat="@drawable/ic_person" />
    <TextView
        android:id="@+id/textView5"
        style="@style/TitleText_detailClient"
        android:text="@string/phone_number1"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:drawableStartCompat="@drawable/ic_phone" />

    <TextView
        android:id="@+id/textView3"
        style="@style/TitleText_detailClient"
        android:text="@string/StateNumberCar1"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:drawableStartCompat="@drawable/ic_country" />

    <TextView
        android:id="@+id/textView4"
        style="@style/TitleText_detailClient"
        android:text="@string/FinalSum1"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:drawableStartCompat="@drawable/ic_cash" />

    <TextView
        android:id="@+id/textView12"
        style="@style/TitleText_detailClient"
        android:text="@string/datePicker"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:drawableStartCompat="@drawable/ic_date" />


    <TextView
        android:id="@+id/textView13"
        style="@style/TitleText_detailClient"
        android:text="@string/Notes1"
        app:layout_constraintEnd_toEndOf="@+id/textView12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:drawableStartCompat="@drawable/ic_task_build" />


    <TextView
        android:id="@+id/carName_cng2"
        style="@style/changeText_detailClient"
        android:layout_marginTop="20dp"
        android:text="Mercedes-benz"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/stateNumber_cng2"
        style="@style/changeText_detailClient"
        android:text="KZ063AMG01"
        app:layout_constraintEnd_toEndOf="@+id/phoneNumber_cng2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/phoneNumber_cng2" />

    <TextView
        android:id="@+id/sum_cng2"
        style="@style/changeText_detailClient"
        android:text="100,000"
        app:layout_constraintEnd_toEndOf="@+id/stateNumber_cng2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/stateNumber_cng2" />

    <TextView
        android:id="@+id/phoneNumber_cng2"
        style="@style/changeText_detailClient"
        android:text="89798516545"
        app:layout_constraintEnd_toEndOf="@+id/clientName_cng2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/clientName_cng2" />

    <TextView
        android:id="@+id/clientName_cng2"
        style="@style/changeText_detailClient"
        android:text="Shevtsov"
        app:layout_constraintEnd_toEndOf="@+id/carName_cng2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/carName_cng2" />
    <TextView
        android:id="@+id/datePicker_cng2"
        style="@style/changeText_detailClient"
        android:text="19 july 2021"
        app:layout_constraintEnd_toEndOf="@+id/sum_cng2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/sum_cng2" />

    <TextView
        android:id="@+id/detail_cng2"
        style="@style/changeText_detailClient"
        android:text="Engine, chassis"
        app:layout_constraintEnd_toEndOf="@+id/datePicker_cng2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/datePicker_cng2" />
</androidx.constraintlayout.widget.ConstraintLayout>