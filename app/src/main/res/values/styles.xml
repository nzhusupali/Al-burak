<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ET_PreOrder_AddCar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:ems">10</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/black
        </item>
    </style>

    <style name="ET_alert_item">
        <item name="android:focusable">false</item>
        <item name="android:enabled">false</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:ems">10</item>
        <item name="android:windowSoftInputMode">stateHidden</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:inputType">textEmailAddress</item>
        <item name="android:background">@drawable/aler_item_background</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/black
        </item>
    </style>

    <style name="ET_end_preOrder">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:ems">10</item>
        <item name="android:windowSoftInputMode">stateHidden</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:inputType">textEmailAddress</item>
        <item name="android:background">@drawable/aler_item_background</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/black
        </item>
    </style>

    <style name="ET_Complete_AddCar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:ems">10</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:inputType">textPersonName</item>
        <item name="android:background">@color/white</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/black
        </item>
    </style>

    <style name="sum_ClientList">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TitleText_detailClient">
        <item name="android:layout_marginTop">18dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="changeText_detailClient">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">22dp</item>
        <item name="android:layout_marginStart">130dp</item>
    </style>

    <style name="btn_alert_edit_item">
        <item name="background">@drawable/aler_item_background</item>
        <item name="android:layout_width">130dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:textColor">@color/ButtonTextColor</item>
    </style>




</resources>